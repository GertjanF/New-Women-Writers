<%= flash_notice %>

<h1><%= link_to "Print:", :action => "index" %>
<%=h @print.publisher %>, <%=h @print.year %> 
</h1>
<div class="details">
<table class="print details">
<tr class="print details"><td class="print details">Work:</td><td class="print details"><% unless @print.work.nil? %><%= link_to h(@print.work.title), work_path(:id => @print.work.id) %><% end %></td></tr>
<tr class="print details"><td class="print details">Year:</td><td class="print details"><%=h @print.year %></td></tr>
<tr class="print details"><td class="print details">Publisher:</td><td class="print details"><%=h @print.publisher %></td></tr>
<tr class="print details"><td class="print details">Location:</td><td class="print details"><%=h @print.location %></td></tr>
<tr class="print details"><td class="print details">Edition:</td><td class="print details"><%=h @print.edition %></td></tr>
	<% if logged_in? %>
		<tr><td colspan='2'><hr class="line" noshade="noshade" /></td></tr>
		<tr class="print details"><td class="print details">Updated at:</td><td class="print details"><%=h @print.updated_at.strftime('%d %B %Y, %H:%M') %> </td></tr>
	<% end %>
</table>

<% if editor? %>
<p class="modify">
	<div>
		<%= edit_button(@print, 'print') %>
<% if admin? %>
		<%= delete_button(@print, 'print') %>
<% end %>
	</div>
</p>
<% end %>
	
</div>