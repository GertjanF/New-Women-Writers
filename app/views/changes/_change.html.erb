<!--- start change -->
<tr>
	<td><%= change.user.name %></td>
	<td><%= link_to h(change.changetype+' '+change.object_name+' (id='+change.object_id.to_s+')'), :controller => change.object_name, :action => "show", :id => change.object_id  %></td>
	<td><%= change.created_at.strftime('%d-%m-%Y %H:%M') %></td>
	<td><% if change.approved == 1 then %>Approved<% else %><% end %>
</tr>

